{
  "name": "@cronflow/core",
  "version": "0.9.0",
  "description": "Native Rust bindings for Cronflow",
  "main": "index.node",
  "types": "index.d.ts",
  "napi": {
    "name": "core",
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-unknown-linux-musl",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-apple-darwin",
        "aarch64-pc-windows-msvc"
      ]
    }
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">= 20"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublish:artifacts": "napi prepublish -t npm"
  },
  "dependencies": {
    "@napi-rs/cli": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dali-benothmen/cronflow.git"
  }
}
